{
    "name": "dillingham/nova-id-link",
    "description": "Links the ID field in Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Dillingham\\NovaIdLink\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dillingham\\NovaIdLink\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
